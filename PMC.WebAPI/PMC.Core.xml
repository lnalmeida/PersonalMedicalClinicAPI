<?xml version="1.0"?>
<doc>
    <assembly>
        <name>PMC.Core</name>
    </assembly>
    <members>
        <member name="T:PMC.Core.Domain.Cliente">
            <summary>
            Objeto cliente.
            </summary>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.Id">
            <summary>
            Id do cliente. Campo numérico inteiro, autoincrementável.
            </summary>
            <example>1</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.Name">
            <summary>
            Nome do cliente.
            </summary> 
            <example>José da Silva</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.BirthDate">
            <summary>
            Data de nascimento do cliente.
            </summary>
            <example>1976-01-01</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.Gender">
            <summary>
            Gênero do cliente, só existem 2.
            </summary>
            <example>M</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.PhoneNumber">
            <summary>
            Telefone do cliente. pode ser no formato (99)99999-9999 ou 9999999999.
            </summary>
            <example>(21)98767-9087</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.Email">
            <summary>
            Email do cliente.
            </summary>
            <example>jose.silva@email.com</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.Document">
            <summary>
            Documento do cliente. RG,CNH ou CPF. Aceita traços e pontos
            </summary>
            <example>024.653.984-85</example>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.CreatedAt">
            <summary>
            Data de criação do registro do cliente. Gerenciuada pela API.
            </summary>
        </member>
        <member name="P:PMC.Core.Domain.Cliente.LastUpdatedAt">
            <summary>
            Data da última atualização no registro do cliente. Gerenciada pela API.
            </summary>
        </member>
    </members>
